{
  "html": {
    "allowed_file_extensions": [
      "htm",
      "html",
      "xhtml",
      "shtml",
      "xml",
      "svg",
      "dust"
    ],
    "brace_style": "expand",
    "end_with_newline": true,
    "indent_char": " ",
    "indent_handlebars": true,
    "indent_inner_html": false,
    "indent_scripts": "keep",
    "indent_size": 2,
    "max_preserve_newlines": 10,
    "indent_inner_html": true,
    "unformatted": [
      "pre",
      "textarea"
    ],
    "wrap_line_length": 72
  }
}
