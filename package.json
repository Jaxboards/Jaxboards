{
    "name": "Jaxboards",
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.26.0",
        "esbuild": "^0.25.3",
        "eslint": "^9.26.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-import-resolver-node": "^0.3.9",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.3",
        "husky": "^9.1.7",
        "js-beautify": "^1.15.4",
        "node-composer-runner": "^0.4.0",
        "prettier": "^3.5.3",
        "stylelint": "^16.16.0",
        "stylelint-config-standard": "^37.0.0",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.31.1"
    },
    "engines": {
        "composer": "2.8.9",
        "node": "^22.14",
        "npm": "^10.9"
    },
    "license": "MIT",
    "private": true,
    "scripts": {
        "autofix": "npm run-script eslint-fix && npm run-script stylelint-fix && npm run-script rector && npm run-script phpcbf && npm run-script php-cs-fixer && npm run-script prettier",
        "build": "node ./esbuild",
        "ci-lint": "npm run-script eslint-report && npm run-script stylelint-report && npm run-script parallel-lint-report && npm run-script phpcs-report && npm run-script phpstan-report && npm run-script psalm-report && npm run-script phpmd-report",
        "eslint": "eslint \"Script/**/*.ts\"",
        "eslint-fix": "eslint \"Script/**/*.ts\" --fix",
        "eslint-report": "eslint \"Script/**/*.ts\" --format json --output-file eslint-report.json",
        "lint": "npm run-script eslint && npm run-script stylelint && npm run-script parallel-lint && npm run-script phpcs && npm run-script phpstan && npm run-script psalm && npm run-script phpmd",
        "parallel-lint": "composer-runner -- run-script --no-interaction parallel-lint",
        "parallel-lint-report": "composer-runner -- run-script --no-interaction parallel-lint-report",
        "php-cs-fixer": "composer-runner -- run-script --no-interaction php-cs-fixer",
        "php-lint": "composer-runner -- run-script --no-interaction lint",
        "phpcbf": "composer-runner -- run-script --no-interaction phpcbf",
        "phpcs": "composer-runner -- run-script --no-interaction phpcs",
        "phpcs-report": "composer-runner -- run-script --no-interaction phpcs-report",
        "phpmd": "composer-runner -- run-script --no-interaction phpmd",
        "phpmd-report": "composer-runner -- run-script --no-interaction phpmd-report",
        "phpstan": "composer-runner -- run-script --no-interaction phpstan",
        "phpstan-report": "composer-runner -- run-script --no-interaction phpstan-report",
        "postinstall": "composer-runner -- install",
        "prettier": "prettier --write --single-quote \"**/*.{{t,j}s{,x,on},{c,sa,sc,le}ss,vue,md,y{,a}ml,graphql}\"",
        "psalm": "composer-runner -- run-script --no-interaction psalm",
        "psalm-report": "composer-runner -- run-script --no-interaction psalm-report",
        "rector": "composer-runner -- run-script --no-interaction rector",
        "stylelint": "stylelint \"./**/*.{c,sa,sc,le}ss\"",
        "stylelint-fix": "stylelint --fix \"./**/*.{c,sa,sc,le}ss\"",
        "stylelint-report": "stylelint \"./**/*.{c,sa,sc,le}ss\" --formatter json --output-file stylelint-report.json",
        "prepare": "husky",
        "test": "composer-runner -- run-script test"
    },
    "type": "module",
    "version": "2.1.5"
}
