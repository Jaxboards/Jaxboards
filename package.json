{
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "eslint": "^9.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "js-beautify": "^1.15.4",
    "node-composer-runner": "^0.4.0",
    "prettier": "^3.5.3",
    "rollup": "^4.36.0",
    "stylelint": "^16.16.0",
    "stylelint-config-standard": "^37.0.0"
  },
  "engines": {
    "composer": "2.8.6",
    "node": "^22.14",
    "npm": "^10.9"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "autofix": "npm run-script prettier && npm run-script eslint-fix && npm run-script stylelint-fix && npm run-script php-fix-cs",
    "build": "rollup -c",
    "ci-lint": "eslint 'Script/**/*.js' --format json --output-file eslint-report.json; stylelint \"./**/*.{c,sa,sc,le}ss\" --formatter json --output-file stylelint-report.json; npm run-script php-report-cs",
    "eslint": "eslint 'Script/**/*.js'",
    "eslint-fix": "eslint 'Script/**/*.js' --fix",
    "php-check-cs": "composer-runner -- run-script --no-interaction check-cs",
    "php-fix-cs": "composer-runner -- run-script --no-interaction fix-cs",
    "php-report-cs": "composer-runner -- run-script --no-interaction report-cs",
    "postinstall": "composer-runner -- install",
    "pre-commit": "npm run-script eslint && npm run-script stylelint && npm run-script php-check-cs",
    "prettier": "prettier --write --single-quote \"**/*.{{t,j}s{,x,on},{c,sa,sc,le}ss,vue,md,y{,a}ml,graphql}\"",
    "stylelint": "stylelint \"./**/*.{c,sa,sc,le}ss\"",
    "stylelint-fix": "stylelint --fix \"./**/*.{c,sa,sc,le}ss\""
  },
  "type": "module"
}
