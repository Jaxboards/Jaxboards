{
  "name": "jaxboards/jaxboards",
  "description": "Realtime forum",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "ACP\\": "ACP",
      "emoticons\\": "emoticons",
      "Jax\\": "Jax",
      "Service\\": "Service",
      "Tools\\": "Tools",
      "Tests\\": "Tests"
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "platform": {
      "composer": "2.9.5",
      "php": "8.5"
    },
    "allow-plugins": {
      "carthage-software/mago": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "brianium/paratest": "^7.16",
    "carthage-software/mago": "^1.8.0",
    "dg/bypass-finals": "^1.9",
    "joeymckenzie/nasastan": "^0.1.2",
    "phpunit/phpunit": "^12.5",
    "phpunit/phpunit-dom-assertions": "^3.7",
    "rector/rector": "^2.3",
    "seanjohnson08/mysqlite": "dev-main@dev",
    "spatie/phpunit-snapshot-assertions": "^5.3"
  },
  "require": {
    "composer": "2.9.5",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-zip": "*",
    "geoip2/geoip2": "^3.3",
    "lib-curl": "*",
    "lib-curl-openssl": "*",
    "lib-openssl": "*",
    "lib-zip-libzip": "*",
    "nesbot/carbon": "^3.11",
    "php": "8.5",
    "php-curl-class/php-curl-class": "^12.0",
    "php-di/php-di": "^7.1",
    "twig/twig": "^3.23"
  },
  "scripts": {
    "analyze": "mago analyze",
    "lint": "mago lint",
    "ci-lint": "php Tools/mago_to_sonar.php",
    "format": "mago format",
    "migrations": "php Tools/migrations.php",
    "rector": "rector",
    "test": [
      "@putenv XDEBUG_MODE=coverage",
      "paratest"
    ]
  },
  "version": "4.0.0"
}
